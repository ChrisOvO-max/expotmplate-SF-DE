{
  "expo": {
    "extra": {
      "eas": {
        "projectId": "4ade83f8-e46b-466e-a0f6-36f938f05ccb"
      }
    },
    "name": "SF-DE",
    "slug": "sf-de",  // 改为与EAS项目一致的slug（小写无空格）
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon_downloaded.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "cli": {
      "appVersionSource": "remote"  // 补充cli配置，消除版本源警告
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow SF-DE to access your camera",
          "microphonePermission": "Allow SF-DE to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
